{
  "name": "headlinehub",
  "version": "1.0.0",
  "description": "A modern news aggregation and scraping service.",
  "main": "app.js",
  "scripts": {
    "build": "tsc",
    "postbuild": "copyfiles -u 2 \"src/generated/prisma/**/*\" dist/generated/",
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "clean": "rimraf dist",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prebuild": "npm run clean",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },
  "keywords": [
    "news",
    "scraper",
    "telegram",
    "bot",
    "prisma",
    "gambia"
  ],
  "author": "Ahmed Eyan Jeng",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "node-telegram-bot-api": "^0.66.0",
    "p-limit": "^6.2.0",
    "pg": "^8.16.3",
    "puppeteer": "^24.15.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/node-telegram-bot-api": "^0.64.9",
    "@types/p-limit": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.31.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "prisma": "^6.12.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
